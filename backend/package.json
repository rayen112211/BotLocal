{
  "name": "botlocal-backend",
  "version": "1.0.0",
  "description": "BotLocal AI WhatsApp Chatbot Backend",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "db:push": "prisma db push --accept-data-loss",
    "db:seed": "ts-node seed.ts",
    "setup": "npm install && npm run db:push && npm run db:seed"
  },
  "keywords": [
    "whatsapp",
    "chatbot",
    "ai",
    "businesses"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@huggingface/inference": "^4.13.12",
    "@langchain/core": "^1.1.27",
    "@langchain/openai": "^1.2.9",
    "@prisma/client": "^5.21.1",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.2.0",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "stripe": "^20.3.1",
    "twilio": "^5.12.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.3.0",
    "nodemon": "^3.1.14",
    "prisma": "^5.21.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}